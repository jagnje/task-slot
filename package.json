{
  "name": "phaser-project-template",
  "version": "4.0.0-rc.5",
  "description": "Phaser 3 starter template with TypeScript and webpack.",
  "homepage": "https://github.com/yandeu/phaser-project-template#readme",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "format": "prettier --check src/scripts/**",
    "format:write": "prettier --write src/scripts/**"
  },
  "keywords": [
    "Phaser",
    "Phaser 4",
    "Phaser4",
    "html5 game",
    "TypeScript",
    "webpack"
  ],
  "author": {
    "name": "Andrej IvanoviÄ",
    "url": "https://github.com/jagnje"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jagnje/task-slot.git"
  },
  "template": {
    "name": "phaser-project-template",
    "url": "https://github.com/yandeu/phaser-project-template",
    "author": "Yannick Deubel (https://github.com/yandeu)"
  },
  "engines": {
    "node": ">=22"
  },
  "license": "MIT",
  "dependencies": {
    "phaser": "4.0.0-rc.5"
  },
  "devDependencies": {
    "@yandeu/prettier-config": "0.0.4",
    "copy-webpack-plugin": "13.0.1",
    "html-webpack-plugin": "5.6.5",
    "javascript-obfuscator": "5.1.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "serve": "14.2.5",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3",
